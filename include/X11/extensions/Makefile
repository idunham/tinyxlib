all::

includes:: $(HEADERS)
	@if [ -d $(BUILDINCDIR)/X11/extensions ]; then \
		set +x; \
	else \
		if [ -h $(BUILDINCDIR)/X11/extensions ]; then \
			(set -x; rm -f $(BUILDINCDIR)/X11/extensions); \
		fi; \
		(set -x; $(MKDIRHIER) $(BUILDINCDIR)/X11/extensions); \
	fi
	@(set -x; cd $(BUILDINCDIR)/X11/extensions && for i in $(HEADERS); do \
		$(RM) $$i; \
		$(LN) $(BUILDINCTOP)/../../$(CURRENT_DIR)/$$i .; \
	done)

install:: $(HEADERS)
	@if [ -d $(DESTDIR)$(INCDIR)/X11/extensions ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(INCDIR)/X11/extensions ]; then \
			(set -x; rm -f $(DESTDIR)$(INCDIR)/X11/extensions); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(INCDIR)/X11/extensions); \
	fi
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; *[i]*) set +e;; esac; done; \
	for i in $(HEADERS); do \
		(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTINCFLAGS) $$i $(DESTDIR)$(INCDIR)/X11/extensions); \
	done

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	$(RM) $@
	 	$(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@

.SUFFIXES: .s

.c.s:
	$(RM) $@
	 	$(CC) -S $(CFLAGS) $(_NOOP_) $*.c

emptyrule::

cleandir::
	$(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut  "#"*

Makefile:: $(IMAKE)

$(IMAKE) $(IMAKE).o:
	-@(cd $(IMAKESRC) && if [ -f Makefile ]; then \
	echo "checking $@ in $(IMAKESRC) first..."; $(MAKE) all; else \
	echo "bootstrapping $@ from Makefile.ini in $(IMAKESRC) first..."; \
	$(MAKE) -f Makefile.ini BOOTSTRAPCFLAGS="$(BOOTSTRAPCFLAGS)"; fi; \
	echo "okay, continuing in $(CURRENT_DIR)")

Makefile::
	-@if [ -f Makefile ]; then set -x; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

man_keywords::

html_index::

clean:: cleandir

distclean:: cleandir

